for %joyo_readings.keys -> $k {
    my $v = %joyo_readings{$k};
    my @vs=split('_',$v);
# Now we split them up in ON and kun, and then filter the Verbs from the kun
    my @on_vs = @vs.grep({$_~~/<[ア..ヺ]>/}) ;  
    my @kun_vs = @vs.grep({$_~~/<[あ..ん]>/}) ;  
    my @verb_vs = @vs.grep({$_~~/<[うむくつるすぬ]>$/}) ;  
# Now we should convert these to romaji
    say "$k => "~@verb_vs if @verb_vs;

}

