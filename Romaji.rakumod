sub katakanaToRomaji(Str $kstr --> Str) is export {

    my @ks = $kstr.comb;
    my @rs = @ks.map({%katakana{$_}});
    my $r_str = @rs.join('');
    while $r_str ~~/_/ {
        for %combined_chars.keys -> $c {
            my $cc = %combined_chars{$c};
            $r_str ~~ s/$c/$cc/;  
        }
    }
    return $r_str.lc;
}

sub hiraganaToRomaji (Str $kstr --> Str) is export  {

    my @ks = $kstr.comb;
    my @rs = @ks.map({%hiragana{$_}});
    my $r_str = @rs.join('');
    while $r_str ~~/_/ {
        for %combined_chars.keys -> $c {
            my $cc = %combined_chars{$c};
            $r_str ~~ s/$c/$cc/;  
        }
    }
    return $r_str.lc;
}


